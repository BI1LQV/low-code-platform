{
  "private": true,
  "packageManager": "pnpm@7.17.1",
  "scripts": {
    "postinstall": "simple-git-hooks",
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "wow": "tokei . -e *.yaml -e **/test/**"
  },
  "dependencies": {
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vueuse/core": "^9.6.0",
    "element-plus": "^2.2.25",
    "pinia": "^2.0.27",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.22.11",
    "vite-plugin-inspect": "^0.7.9",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@bi1lqv/eslint-config": "^0.26.11",
    "@iconify-json/carbon": "^1.1.11",
    "@types/node": "^18.11.9",
    "@unocss/reset": "^0.47.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue-macros/volar": "^0.5.4",
    "@vue/test-utils": "^2.2.4",
    "eslint": "^8.28.0",
    "esno": "^0.16.3",
    "jsdom": "^20.0.3",
    "picocolors": "^1.0.0",
    "pnpm": "^7.17.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.3",
    "unocss": "^0.47.1",
    "unplugin-vue-macros": "^1.0.3",
    "vite": "^3.1.3",
    "vite-plugin-hot-export": "^0.5.9",
    "vite-plugin-info": "^0.3.1",
    "vite-plugin-pages": "^0.27.1",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint && pnpm typecheck",
    "commit-msg": "pnpm exec esno scripts/verifyCommit.ts $1"
  }
}
